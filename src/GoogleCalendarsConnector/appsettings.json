{
    "Serilog": {
        "LogDirectory": "./logs"
    },
    "StreamGroupTracker": {
        "IntervalSec": 15,
        "MinPedestrians": 1,
        "MinFaces": 0
    },
    "Config": {
        "MaxParallelActionBlocks": 4
    },
    "Source": {
        "GraphQL": {
            "Schema": "ws",
            "Host": "localhost",
            "Port": "8096",
            "Path": "graphql"
        }
    },
    "GoogleCalendar": {
        "CredentialsPath": "credentials.json",
        "TokenPath": "token.json",
        "DefaultTimeZone": "Europe/Bratislava",
        "DefaultEventDurationHours": 1,
        "MeetingDurationMin": 30,
        "ServiceUser": "service@company.com"
    },
    "CalendarCache": {
        "ExpirationMinutes": 30,
        "MaxSize": 1000
    },
    "StreamGroupsMapping": [
        {
            "GroupName": "Group1",
            "CalendarId": "x@resource.calendar.google.com"
        }
    ]
}